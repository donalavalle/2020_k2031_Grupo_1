  179 lista_declaraciones: declaracion
  180                    | lista_declaraciones declaracion

  181 lista_sentencias: sentencia
  182                 | lista_sentencias sentencia

  183 sentencia_seleccion: IF '(' expresion ')' sentencia
  184                    | IF '(' expresion ')' sentencia ELSE sentencia
  185                    | SWITCH '(' expresion ')' sentencia

  186 sentencia_iteracion: WHILE '(' expresion ')' sentencia
  187                    | DO sentencia WHILE '(' expresion ')' ';'
  188                    | FOR '(' expresion_opc ';' expresion_opc ';' expresion_opc ')' sentencia

  189 sentencia_etiquetada: CASE exp_constante ':' sentencia
  190                     | DEFAULT ':' sentencia
  191                     | ID ':' sentencia

  192 sentencia_salto: CONTINUE_BREAK ';'

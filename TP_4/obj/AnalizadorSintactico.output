  159                             | declarador_abstracto_directo_opc '(' lista_tipos_param_opc ')'

  160 declarador_abstracto_directo_opc: /* vacío */
  161                                 | declarador_abstracto_directo

  162 lista_tipos_param_opc: /* vacío */

  121 decla: puntero_opc declarador_directo

  122 puntero_opc: /* vacío */
  123            | puntero

  124 puntero: '*' lista_calificadores_tipos_opc
  125        | '*' lista_calificadores_tipos_opc puntero

  126 lista_calificadores_tipos_opc: /* vacío */
  127                              | lista_calificadores_tipos

  128 lista_calificadores_tipos: CALIF_TIPO
  129                          | lista_calificadores_tipos CALIF_TIPO

  130 declarador_directo: ID
  131                   | '(' decla ')'
  132                   | declarador_directo '[' exp_constante_opc ']'
  133                   | declarador_directo '(' lista_tipos_param_opc ')'
  134                   | declarador_directo '(' lista_identificadores_opc ')'

  135 lista_identificadores_opc: /* vacío */
  136                          | lista_identificadores

  137 lista_tipos_param: lista_parametros

  138 lista_parametros: declaracion_parametro
  139                 | lista_parametros ',' declaracion_parametro

  140 declaracion_parametro: especificadores_declaracion decla
  141                      | especificadores_declaracion declarador_abstracto_opc

  142 declarador_abstracto_opc: /* vacío */
  143                         | declarador_abstracto

  144 lista_identificadores: ID
  145                      | lista_identificadores ',' ID

  146 especificador_enum: ENUM ID_opc '{' lista_enumeradores '}'
  147                   | ENUM ID

  148 lista_enumeradores: enumerador
  149                   | lista_enumeradores ',' enumerador

  150 enumerador: const_de_enumeracion
  151           | const_de_enumeracion '=' exp_constante

  152 const_de_enumeracion: ID

  153 nombre_tipo: lista_calificadores declarador_abstracto_opc

  154 declarador_abstracto: puntero
  155                     | puntero_opc declarador_abstracto_directo

  156 declarador_abstracto_directo: '(' declarador_abstracto ')'
  157                             | declarador_abstracto_directo_opc '[' exp_constante_opc ']'
  158                             | declarador_abstracto_directo_opc '(' lista_tipos_param_opc ')'

  159 declarador_abstracto_directo_opc: /* vacío */
  160                                 | declarador_abstracto_directo

  161 lista_tipos_param_opc: /* vacío */
  162                      | lista_tipos_param

  163 sentencia: sentencia_exp
  164          | sentencia_compuesta
  165          | sentencia_seleccion
  166          | sentencia_iteracion
  167          | sentencia_etiquetada
  168          | sentencia_salto
  169          | especificadores_declaracion decla sentencia_compuesta

  170 sentencia_exp: expresion_opc ';'

  171 expresion_opc: /* vacío */
  172              | expresion

  173 sentencia_compuesta: '{' lista_declaraciones_opc lista_sentencias_opc '}'

  174 lista_declaraciones_opc: /* vacío */
  175                        | lista_declaraciones

  176 lista_sentencias_opc: /* vacío */
  177                     | lista_sentencias

  178 lista_declaraciones: declaracion
  179                    | lista_declaraciones declaracion

  180 lista_sentencias: sentencia
  181                 | lista_sentencias sentencia

  182 sentencia_seleccion: IF '(' expresion ')' sentencia
  183                    | IF '(' expresion ')' sentencia ELSE sentencia
  184                    | SWITCH '(' expresion ')' sentencia

  185 sentencia_iteracion: WHILE '(' expresion ')' sentencia
  186                    | DO sentencia WHILE '(' expresion ')' ';'
  187                    | FOR '(' expresion_opc ';' expresion_opc ';' expresion_opc ')' sentencia

  188 sentencia_etiquetada: CASE exp_constante ':' sentencia
  189                     | DEFAULT ':' sentencia
  190                     | ID ':' sentencia

  191 sentencia_salto: CONTINUE_BREAK ';'
  192                | RETURN expresion_opc ';'
  193                | GOTO ID ';'



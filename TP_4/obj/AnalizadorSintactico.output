   48         | exp_desp OP_DESP_DER exp_aditiva

   49 exp_aditiva: exp_multip
   50            | exp_aditiva '+' exp_multip
   51            | exp_aditiva '-' exp_multip

   52 exp_multip: exp_conversion
   53           | exp_multip '*' exp_conversion
   54           | exp_multip '/' exp_conversion
   55           | exp_multip '%' exp_conversion

   56 exp_conversion: exp_unaria
   57               | '(' nombre_tipo ')' exp_conversion exp_unaria

   58 exp_unaria: exp_sufijo
   59           | OP_INC exp_unaria
   60           | OP_DEC exp_unaria
   61           | op_unario exp_conversion
   62           | SIZEOF exp_unaria
   63           | SIZEOF '(' nombre_tipo ')'

   64 op_unario: '&'
   65          | '*'
   66          | '+'
   67          | '-'
   68          | '~'
   69          | '!'

   70 exp_sufijo: exp_primaria
   71           | exp_sufijo '[' expresion ']'
   72           | exp_sufijo '(' lista_argumentos ')'
   73           | exp_sufijo '.' ID
   74           | exp_sufijo OP_MIEMBRO_PUNT ID
   75           | exp_sufijo OP_INC
   76           | exp_sufijo OP_DEC

   77 lista_argumentos: exp_asignacion
   78                 | lista_argumentos ',' exp_asignacion

   79 exp_primaria: const
   80             | STRING
   81             | '(' expresion ')'

   82 declaracion: especificadores_declaracion lista_declaradores

   83 especificadores_declaracion: CLASE_ALM especificadores_declaracion_opc
   84                            | especificador_tipo especificadores_declaracion_opc
   85                            | CALIF_TIPO especificadores_declaracion_opc

   86 especificadores_declaracion_opc: /* vacío */
   87                                | especificadores_declaracion

   88 lista_declaradores: declarador
   89                   | declarador ',' lista_declaradores

   90 declarador: decla
   91           | decla '=' inicializador

   92 inicializador: exp_asignacion
   93              | '{' lista_inicializadores coma_opc '}'

   94 coma_opc: /* vacío */

   96 lista_inicializadores: inicializador
   97                      | lista_inicializadores ',' inicializador

   98 especificador_tipo: TIPO_DATO
   99                   | especificador_struct_union
  100                   | especificador_enum

  101 especificador_struct_union: STRUCT_UNION ID_opc '{' lista_declaradores_struct '}'
  102                           | STRUCT_UNION ID

  103 ID_opc: /* vacío */
  104       | ID

  105 lista_declaradores_struct: declaracion_struct
  106                          | lista_declaradores_struct declaracion_struct

  107 declaracion_struct: lista_calificadores declaradores_struct ';'

  108 lista_calificadores: especificador_tipo lista_calificadores_opc
  109                    | CALIF_TIPO lista_calificadores_opc

  110 lista_calificadores_opc: /* vacío */

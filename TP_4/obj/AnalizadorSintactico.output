   13 exp_asignacion: exp_condicional
   14               | exp_unaria op_asignacion exp_asignacion

   15 exp_condicional: exp_OR_log
   16                | exp_OR_log '?' expresion ':' exp_condicional

   17 op_asignacion: '='
   18              | ASIG_MULT
